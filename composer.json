{
    "name": "friendsofcake/crud-demo-app",
    "description": "CakePHP + CRUD demo app",
    "homepage": "http://friendsofcake.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.4.19",
        "cakephp/cakephp": "3.0.*-dev",
        "phpunit/phpunit": "3.7.*",
        "mobiledetect/mobiledetectlib": "2.*",

        "friendsofcake/crud": "dev-cake3",
        "friendsofcake/crud-view": "dev-master",
        "friendsofcake/search": "dev-master"
    },
    "require-dev": {
        "d11wtq/boris": "1.0.*",
        "friendsofcake/cakephp-test-utilities": "dev-master"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install."
    },
    "autoload": {
        "psr-4": {
            "App\\": "App",
            "App\\Test\\": "Test",
            "": "./Plugin"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall"
    }
}
